<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.1"?>
<plugin>
   <extension
         id="robot"
         name="Example Robot"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run class="org.eclipse.ecf.example.clients.RobotApplication"/>
      </application>
   </extension>
   <extension
         id="sorobot"
         name="Example SO Robot"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run class="org.eclipse.ecf.example.clients.XMPPSORobotApplication"/>
      </application>
   </extension>
   <extension
         id="filetransferrobot"
         name="Example XMPP File Transfer"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run class="org.eclipse.ecf.example.clients.XMPPFileTransferApplication"/>
      </application>
   </extension>

</plugin>
