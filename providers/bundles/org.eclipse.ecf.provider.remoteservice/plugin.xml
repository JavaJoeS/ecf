            adaptableType="org.eclipse.ecf.provider.generic.TCPClientSOContainer"
            class="org.eclipse.ecf.provider.remoteservice.generic.RemoteServiceContainerAdapterFactory">
         <adapter type="org.eclipse.ecf.remoteservice.IRemoteServiceContainer"/>
      </factory>
   </extension>
   <extension
         name="ecf.generic.remoteservice"
         point="org.eclipse.ecf.containerFactory">
      <containerFactory
            class="org.eclipse.ecf.provider.remoteservice.generic.RemoteServiceContainerFactory"
            name="ecf.generic.remoteservice"
            description="ECF Generic Remoteservice" />
   </extension>

</plugin>
