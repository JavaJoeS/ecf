<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.1"?>
<plugin>
   <extension
         point="org.eclipse.ecf.containerFactory">
      <containerFactory
            class="org.eclipse.ecf.provider.datashare.DatashareContainerInstantiator"
            description="ECF Generic Channel"
            name="ecf.generic.channel"/>
   </extension>
   <extension
         point="org.eclipse.ecf.sharedObjectFactory">
      <sharedObjectFactory
            class="org.eclipse.ecf.provider.datashare.ChannelFactory"
            name="ecf.provider.datashare.channelimpl"/>
   </extension>
   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="org.eclipse.ecf.provider.generic.TCPClientSOContainer"
            class="org.eclipse.ecf.provider.datashare.DatashareContainerAdapterFactory">
         <adapter type="org.eclipse.ecf.datashare.IChannelContainer"/>
      </factory>
   </extension>

</plugin>
